<div id="ribbits" class="panel left">
	<h1>
    <%= title %>
    | <a href=<%='/usr/'+username%>>Profile</a>
  </h1>
    <% for(var i=0; i < follow.length; i++) { %>
	    <div class="ribbitWrapper">
	    	<img class="avatar" src="/gfx/user2.png">
	    	<span class="name">
                <a href="/usr/<%= follow[i] %>">
                    <%= follow[i] %>
                </a>
            </span>
               <span class="time">&nbsp;</span>
      <p>&nbsp;</p>
	    </div>
    <% } %>
</div>
